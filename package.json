{
  "name": "@enso/mcp-server",
  "version": "1.0.0",
  "description": "MCP server for Enso documentation with AI integration",
  "type": "module",
  "main": "dist/index.js",
  "module": "src/index.ts",
  "bin": {
    "enso-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --format esm",
    "dev": "bun src/index.ts",
    "start": "bun dist/index.js",
    "vercel-build": "bun run build",
    "test": "bun test"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@vercel/mcp-adapter": "^0.11.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "mcp",
    "enso",
    "documentation",
    "ai",
    "mintlify"
  ],
  "author": "bezata",
  "license": "MIT"
}
